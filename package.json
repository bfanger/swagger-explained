{
  "name": "swagger-explained",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "svelte-kit sync && concurrently -c \"#c596c7\",\"#676778\",\"#7c7cea\" --kill-others-on-fail \"npm:lint:*\"",
    "lint:prettier": "prettier --check \"src/**/*.svelte\"",
    "lint:svelte-check": "svelte-check --fail-on-warnings --ignore build,package",
    "lint:eslint": "eslint --ext=js,ts,svelte --max-warnings=0 src",
    "format": "eslint --ext=js,ts --fix src && prettier --write src",
    "test": "vitest run",
    "preinstall": "npx only-allow pnpm",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings 0 --ext ts --no-ignore",
      "sh -c 'tsc -p tsconfig.json --noEmit'"
    ],
    "*.js": [
      "eslint --max-warnings 0 --no-ignore"
    ],
    "*.svelte": [
      "svelte-check --fail-on-warnings",
      "prettier --check"
    ]
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.24.0",
    "@types/marked": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-triple": "^1.1.3",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.33.0",
    "git-directory-deploy": "^1.5.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.66.1",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-preprocess": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "marked": "^7.0.5",
    "yaml": "^2.3.2"
  }
}
