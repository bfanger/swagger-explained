{
  "name": "swagger-explained",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "svelte-kit sync && concurrently -c \"#c596c7\",\"#676778\",\"#3074c0\",\"#7c7cea\" --kill-others-on-fail \"npm:lint:*\"",
    "lint:prettier": "prettier --check --loglevel=warn \"src/**/*.svelte\"",
    "lint:svelte-check": "svelte-check --fail-on-warnings --ignore build,package",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext=js,ts,svelte --max-warnings=0 src",
    "format": "prettier --write src && eslint --ext=js,ts --fix src",
    "test": "vitest run",
    "preinstall": "npx only-allow pnpm",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings 0 --ext ts --no-ignore",
      "sh -c 'tsc -p tsconfig.json --noEmit'"
    ],
    "*.js": [
      "eslint --max-warnings 0 --no-ignore"
    ],
    "*.svelte": [
      "svelte-check --fail-on-warnings",
      "prettier --check"
    ]
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.15.2",
    "@types/marked": "^4.0.8",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-triple": "^0.5.8",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "git-directory-deploy": "^1.5.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "sass": "^1.61.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "dependencies": {
    "marked": "^4.3.0",
    "yaml": "^2.2.1"
  }
}
