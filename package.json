{
  "name": "swagger-explained",
  "version": "1.0.2",
  "description": "Browse the swagger specification visually via the json",
  "main": "index.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "test": "npm run lint",
    "lint": "eslint --max-warnings 0 --ext .js,.vue src",
    "deploy": "vue-cli-service build --modern && push-dir --dir dist --branch gh-pages"
  },
  "author": "Bob Fanger <bfanger@gmail.com>",
  "license": "MIT",
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/recommended",
      "@vue/prettier"
    ],
    "plugins": [
      "only-warn"
    ]
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "eslint": "6",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "marked": "^2.1.3",
    "prettier": "^2.3.2",
    "push-dir": "^0.4.1",
    "sass": "^1.36.0",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "dependencies": {
    "sass-loader": "^10.2.0"
  }
}
